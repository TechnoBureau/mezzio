<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TechnoBureau\User\Entity\OauthScopes" table="oauth_scopes" repository-class="TechnoBureau\User\Repository\OauthScopesRepository">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="oauth_scopes_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="scope" type="string" column="scope" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="authCode" target-entity="TechnoBureau\User\Entity\OauthAuthCodes" mapped-by="scope" fetch="LAZY"/>
    <many-to-many field="accessToken" target-entity="TechnoBureau\User\Entity\OauthAccessTokens" mapped-by="scope" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
