<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TechnoBureau\User\Entity\AuthUser" table="auth_user">
    <indexes>
      <index name="auth_user_username_6821ab7c_like" columns="username"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="auth_user_username_key" columns="username"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="auth_user_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="password" type="string" column="password" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastLogin" type="datetimetz" column="last_login" nullable="true"/>
    <field name="isSuperuser" type="boolean" column="is_superuser" nullable="false"/>
    <field name="username" type="string" column="username" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="254" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isStaff" type="boolean" column="is_staff" nullable="false"/>
    <field name="isActive" type="boolean" column="is_active" nullable="false"/>
    <field name="dateJoined" type="datetimetz" column="date_joined" nullable="false"/>
  </entity>
</doctrine-mapping>
