<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TechnoBureau\User\Entity\AuthUserUserPermissions" table="auth_user_user_permissions">
    <indexes>
      <index name="auth_user_user_permissions_user_id_a95ead1b" columns="user_id"/>
      <index name="auth_user_user_permissions_permission_id_1fbb5f2c" columns="permission_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="auth_user_user_permissions_user_id_permission_id_14a6b632_uniq" columns="user_id,permission_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="auth_user_user_permissions_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <many-to-one field="permission" target-entity="TechnoBureau\User\Entity\AuthPermission" fetch="LAZY">
      <join-columns>
        <join-column name="permission_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="TechnoBureau\User\Entity\AuthUser" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
