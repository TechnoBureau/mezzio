<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TechnoBureau\User\Entity\AuthGroupPermissions" table="auth_group_permissions">
    <indexes>
      <index name="auth_group_permissions_permission_id_84c5c92e" columns="permission_id"/>
      <index name="auth_group_permissions_group_id_b120cbf9" columns="group_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="auth_group_permissions_group_id_permission_id_0cd325b0_uniq" columns="group_id,permission_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="auth_group_permissions_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <many-to-one field="permission" target-entity="TechnoBureau\User\Entity\AuthPermission" fetch="LAZY">
      <join-columns>
        <join-column name="permission_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="group" target-entity="TechnoBureau\User\Entity\AuthGroup" fetch="LAZY">
      <join-columns>
        <join-column name="group_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
